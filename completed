<script>
try{document.domain="sina.com.cn";}catch(e){}
/*序号切换,下一个,自动播放*/
var Nexts = (function(){
    var pro = Nexts.prototype={
        init : function(args){
            // 初始序号
            this.index = args.index || 0;
            // 上一个序号
            this.pre_index = 0;
            // 下一个序号
            this.next_index = 0;
            // 长度
            this.length = args.length || 0;
            // 时间间隔
            this.interval = args.interval || 3000 ;
            // 上一个动作
            this.last_action= args.last_action || function(){};
            // 下一个动作
            this.cur_action = args.cur_action || function (){};
            // 初始化动作
            args.init && args.init.call(this);
            this.start();
        },
        // 切换
        change : function(index){
            this.next_index =index;
            this.last_action.call(this);
            this.pre_index = this.index;
            this.index = index;
            this.cur_action.call(this);
            return this;
        },
        // 高级切换
        changeA : function(last_action ,cur_action ,index){
            last_action.call(this);
            this.index = index;
            cur_action.call(this);
            return this;
        },
        // 上一个
        pre : function (){
            return this.change(this.index ? this.index-1 : this.length-1) ;
        },
        // 下一个
        next : function(){
            return this.change((this.index+1) % this.length);
        },
        // 自动播放动作
        start_action : function(){
            this.next();
            this.start();
        },
        // 开始自动播放
        start : function(){
            var self = this;
            this.stop();
            this._sto = setTimeout(function(){
                self.start_action();
            },this.interval);
            return this;
        },
        // 停止
        stop : function(){
            clearTimeout(this._sto);
            return this;
        },
        // 重置自动播放 (时间重新倒计时)
        reset : function(){
            this.stop();
            return this.start();
        }
    };
    pro.init.prototype = pro;

    function Nexts(args){
        return new pro.init(args);
    }

    return Nexts;
})();
</script>
<!-- <script src="js/Nexts.js"></script> -->
<!-- <script src="js/script.js"></script> -->
<script>
    ~function(){
        var $li = $('.box2 .barbox li'),
        $curli = $li.last();
        $('.box2 .navbox li').mouseenter(function(){
            $curli.css({left : $(this).index() * $li.outerWidth()});
        }).click(function(){
            $('html,body').animate({scrollTop:$('.title'+($(this).index()+1)).offset().top});
            return false;
        })
    }();
</script>

