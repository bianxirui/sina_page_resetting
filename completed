        <script src="http://guangdong.sinaimg.cn/images/2013index/slide.min.js" charset="gb2312"></script>
        <script>
        /*通用绑定事件方法 addEvent  */
        var addEvent = function(elm, evType, fn, useCapture) {
            if (elm.addEventListener) {
                elm.addEventListener(evType, fn, useCapture); //DOM2.0
                return true;
            } else if (elm.attachEvent) {
                var r = elm.attachEvent('on' + evType, fn); //IE5+
                return r;
            } else {
                elm['on' + evType] = fn; //DOM 0
            }
        };
        /* 头屏大切换 */
        var headSlide = new Focus.ui.Carousel($(".part4 .navbox li"), $("#head-slides .head-slide"), "current", {
            prev: $("#head-slide-prev"),
            next: $("#head-slide-next"),
            event: "click",
            interval: 5000,
            duration: 2000,
            auto: Boolean(0)
        });
        headSlide.pause();
        //自动替换左右按钮的文字
        var headSlideTextArr = [],
            headSlideTextCtrls = headSlide.ctrls,
            headSlideNext = headSlide.options.next,
            headSlideNextText = Focus(".f18px", headSlideNext),
            headSlidePrev = headSlide.options.prev,
            headSlidePrevText = Focus(".f18px", headSlidePrev);
        var makeIndexCorrect = function(index) {
            var tmpIndex = 0,
                headslideLength = headSlide.total;
            tmpIndex = (index < 0 ? index + headslideLength : index);
            tmpIndex = (tmpIndex >= headslideLength ? tmpIndex - headslideLength : tmpIndex);
            return tmpIndex;
        };
        var changeHeadSlideBtnText = function() {
            // $('.part4 .navbox li').eq(headSlide.current).addClass('active').siblings().removeClass('active');
            headSlidePrevText[0].innerHTML = headSlideTextArr[makeIndexCorrect(headSlide.current - 1)];
            headSlideNextText[0].innerHTML = headSlideTextArr[makeIndexCorrect(headSlide.current + 1)];
        };
        for (i = 0, len = headSlideTextCtrls.length; i < len; i++) {
            headSlideTextArr.push(headSlideTextCtrls[i].innerHTML);
            addEvent(headSlideTextCtrls[i], "click", changeHeadSlideBtnText);
        }
        addEvent(headSlidePrev, "click", changeHeadSlideBtnText);
        addEvent(headSlideNext, "click", changeHeadSlideBtnText);
        headSlide.cycle = function(e) {
            var t = this;
            if (!e) t.paused = false;
            if (t.options.auto) {
                var optionsGap = t.current === 1 ? t.options.interval * 0 : t.options.interval;
                optionsGap = t.current === 0 ? 10000 : optionsGap;
                t.interval = setTimeout(function() { ////开始自动循环
                        t.next();
                    },
                    optionsGap
                );
            }
            return this;
        };
        if (headSlide.options.auto) {
            headSlide.cycle();
        }
        </script>

    </div>

